<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.campushelper"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />

    <application
        android:name="com.example.campushelper.util.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.campushelper.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.campushelper.activity.LoginActivity"></activity>
        <activity android:name="com.example.campushelper.activity.ScheduleActivity">   
        </activity>
        <activity android:name="com.example.campushelper.activity.ScheduleShowActivity"></activity>
        <activity android:name="com.example.campushelper.activity.NoticeActivity"></activity>
        <activity android:name="com.example.campushelper.activity.NoticeShowActivity"></activity>
        <activity android:name="com.example.campushelper.activity.BacklogActivity"></activity>
        <activity android:name="com.example.campushelper.activity.RegisterActivity"></activity>
    </application>

</manifest>
